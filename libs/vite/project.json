{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/vite/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/vite",
        "main": "libs/vite/src/index.ts",
        "tsConfig": "libs/vite/tsconfig.lib.json",
        "assets": [
          "libs/vite/*.md",
          {
            "input": "./libs/vite/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/vite/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/vite",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/vite",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/vite/**/*.ts",
          "libs/vite/generators.json",
          "libs/vite/executors.json",
          "libs/vite/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/vite"],
      "options": {
        "jestConfig": "libs/vite/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "angular"
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": []
}
