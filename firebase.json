{
  "hosting": {
    "public": "dist/apps/test-app",
    "ignore": ["**/.firebase/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "handler"
      }
    ]
  },
  "functions": [
    {
      "source": "dist/apps/test-app",
      "runtime": "nodejs16"
    }
  ],
  "emulators": {
    "auth": {
      "port": 9099
    },
    "ui": {
      "enabled": true
    }
  }
}
